					xmlns:drx="clr-namespace:Dwares.Druid.UI.Xaml;assembly=Druid"
					x:Class="Drive.Themes.Material.MaterialPalette"
					>

	<x:String x:Key="ColorPaletteName">Material.2014</x:String>
	<x:String x:Key="ColorPaletteDesign">Material</x:String>

	<Color x:Key="Blue Gray:50">#ECEFF1</Color>
	<Color x:Key="Blue Gray:100">#CFD8DC</Color>
	<Color x:Key="Blue Gray:200">#B0BEC5</Color>

	<Color x:Key="Cyan">#4DD0E1</Color>
	<Color x:Key="Cyan:300">#00BCD4</Color>
	<Color x:Key="Cyan:500">#00BCD4</Color>
	
	<Color x:Key="Teal">#009688</Color>
	<Color x:Key="Teal:100">#B2DFDB</Color>
	<Color x:Key="Teal:500">#009688</Color>

	<Color x:Key="Gray">#9E9E9E</Color>
	<Color x:Key="Gray:500">#9E9E9E</Color>

</ResourceDictionary>
	